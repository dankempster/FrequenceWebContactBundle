{
    "name": "frequence-web/contact-bundle",
    "type": "symfony-bundle",
    "description": "Event-based contact bundle",
    "keywords": ["contact", "bundle", "event"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yohan Giarelli",
            "homepage": "http://github.com/yohang"
        }
    ],
    "require": {
        "php":                      ">5.3.2",
        "symfony/framework-bundle": ">=2.1,<2.4-dev",
        "symfony/form":             ">=2.1,<2.4-dev",
        "symfony/twig-bundle":      ">=2.1,<2.4-dev",
        "symfony/validator":        ">=2.1,<2.4-dev"
    },
    "require-dev": {
        "phpunit/phpunit":             "3.*@stable",
        "symfony/browser-kit":         ">=2.1,<2.4-dev",
        "symfony/css-selector":        ">=2.1,<2.4-dev",
        "symfony/class-loader":        ">=2.1,<2.4-dev",
        "symfony/dom-crawler":         ">=2.1,<2.4-dev",
        "symfony/finder":              ">=2.1,<2.4-dev",
        "symfony/swiftmailer-bundle":  ">=2.1,<2.4-dev"
    },
    "suggest": {
        "symfony/swiftmailer-bundle": "If your want to use the (default) mail listener."
    },
    "autoload": {
        "psr-0": { "FrequenceWeb\\Bundle\\ContactBundle": "" }
    },
    "target-dir": "FrequenceWeb/Bundle/ContactBundle"
}
